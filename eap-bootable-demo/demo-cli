1. mvn clean package -Pbootable-jar
2. java -jar target/xp20-demo-2.0.0.GA-bootable.jar
3. curl http://localhost:8080/api/greeting
4. curl http://localhost:8080/api/greeting\?name\=Bootable
5. curl http://localhost:9990/health/ready
6. curl http://localhost:9990/health/live

def print_numbers():
  number = 2

  while number <=100:
    if number % 2 == 0:
      print(number)

    number += 1


def print_numbers():
  number = 2

  while number <= 100:
    print(number)

    number += 2
